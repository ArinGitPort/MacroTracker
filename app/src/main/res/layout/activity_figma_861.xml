<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:background="#FFFFFF"
	android:orientation="vertical"
	android:layout_width="match_parent"
	android:layout_height="match_parent">
	<ScrollView
		android:id="@+id/rfs5oz6ou7rp"
		android:layout_width="match_parent"
		android:layout_height="0dp"
		android:layout_weight="1"
		android:background="#000000"
		android:paddingTop="9dp">
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:orientation="vertical">
			<LinearLayout
				android:id="@+id/rwvbtqq2b4j"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:gravity="center_vertical"
				android:background="@drawable/s1a1a1asw16cr39b2e2e2e"
				android:paddingVertical="16dp"
				android:paddingHorizontal="7dp"
				android:layout_marginBottom="5dp"
				android:layout_marginHorizontal="12dp"
				android:orientation="horizontal">

				<LinearLayout
					android:id="@+id/caloriesCounterView"
					android:layout_width="127dp"
					android:layout_height="wrap_content"
					android:background="@drawable/cr40b2e2e2e"
					android:gravity="center_horizontal"
					android:orientation="vertical"
					android:paddingTop="11dp"
					android:layout_marginLeft="20dp"
					android:paddingBottom="56dp">

					<TextView
						android:id="@+id/rs3emzph4pe"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginBottom="32dp"
						android:text="Calories"
						android:textColor="#FFFFFF"
						android:textSize="12sp" />

					<TextView
						android:id="@+id/calorieCount"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="2100"
						android:textColor="#FFFFFF"
						android:textSize="20sp" />
				</LinearLayout>

				<View
					android:layout_width="0dp"
					android:layout_height="0dp"
					android:layout_weight="1"
				 />
				<LinearLayout
					android:id="@+id/remainingMacrosCounterView"
					android:layout_width="199dp"
					android:layout_height="wrap_content"
					android:background="@drawable/cr40b2e2e2e"
					android:paddingVertical="11dp"
					android:paddingHorizontal="34dp"
					android:layout_marginRight="10dp"
					android:orientation="vertical">
					<TextView
						android:id="@+id/rwo0fgj0ko2j"
						android:textColor="#FFFFFF"
						android:textSize="14sp"
						android:layout_marginBottom="14dp"
						android:layout_height="wrap_content"
						android:layout_width="wrap_content"
						android:text="Remaining Macros"
					/>
					<LinearLayout
						android:id="@+id/rpurz0413ejk"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:gravity="center_vertical"
						android:layout_marginBottom="12dp"
						android:orientation="horizontal">
						<TextView
							android:id="@+id/r5ebo2z6muzb"
							android:textColor="#FFFFFF"
							android:textSize="11sp"
							android:layout_marginEnd="4dp"
							android:layout_height="wrap_content"
							android:layout_width="match_parent"
							android:layout_weight="1"
							android:text="Carbs"
						/>
						<TextView
							android:id="@+id/carbsCount"
							android:textColor="#FFFFFF"
							android:textSize="11sp"
							android:textStyle="bold"
							android:layout_height="wrap_content"
							android:layout_width="wrap_content"
							android:text="120"
						/>
					</LinearLayout>
					<LinearLayout
						android:id="@+id/r5n5a7zg4ay"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:gravity="center_vertical"
						android:layout_marginBottom="12dp"
						android:orientation="horizontal">
						<TextView
							android:id="@+id/rcvme6eytm7n"
							android:textColor="#FFFFFF"
							android:textSize="11sp"
							android:layout_marginEnd="4dp"
							android:layout_height="wrap_content"
							android:layout_width="match_parent"
							android:layout_weight="1"
							android:text="Fibers"
						/>
						<TextView
							android:id="@+id/fibersCount"
							android:textColor="#FFFFFF"
							android:textSize="12sp"
							android:textStyle="bold"
							android:layout_height="wrap_content"
							android:layout_width="wrap_content"
							android:text="90"
						/>
					</LinearLayout>
					<LinearLayout
						android:id="@+id/rurxdmreerws"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:gravity="center_vertical"
						android:layout_marginBottom="12dp"
						android:orientation="horizontal">
						<TextView
							android:id="@+id/rok4e8p1d3l"
							android:textColor="#FFFFFF"
							android:textSize="11sp"
							android:layout_height="wrap_content"
							android:layout_width="wrap_content"
							android:text="Protein"
						/>
						<View
							android:layout_width="0dp"
							android:layout_height="0dp"
							android:layout_weight="1"
						 />
						<TextView
							android:id="@+id/proteinCount"
							android:textColor="#FFFFFF"
							android:textSize="10sp"
							android:layout_height="wrap_content"
							android:layout_width="wrap_content"
							android:text="100"
						/>
					</LinearLayout>
					<LinearLayout
						android:id="@+id/rgpndwg96bnt"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:gravity="center_vertical"
						android:orientation="horizontal">
						<TextView
							android:id="@+id/rxcj3edwn5g9"
							android:textColor="#FFFFFF"
							android:textSize="11sp"
							android:layout_marginEnd="4dp"
							android:layout_height="wrap_content"
							android:layout_width="match_parent"
							android:layout_weight="1"
							android:text="Fat"
						/>
						<TextView
							android:id="@+id/fatCount"
							android:textColor="#FFFFFF"
							android:textSize="11sp"
							android:textStyle="bold"
							android:layout_height="wrap_content"
							android:layout_width="wrap_content"
							android:text="80"
						/>
					</LinearLayout>
				</LinearLayout>
			</LinearLayout>

			<LinearLayout
				android:id="@+id/searchBarParent"
				android:layout_width="match_parent"
				android:layout_height="36dp"
				android:layout_marginHorizontal="12dp"
				android:layout_marginBottom="5dp"
				android:background="@drawable/s1a1a1asw8cr23b2e2e2e"
				android:orientation="vertical"
				android:paddingHorizontal="7dp">

				<LinearLayout
					android:id="@+id/searchBar"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="7dp"
					android:background="@drawable/cr14b2e2e2e"
					android:gravity="center_vertical"
					android:orientation="horizontal"
					android:paddingHorizontal="7dp">

					<LinearLayout
						android:id="@+id/rqyvjxty5lnl"
						android:layout_width="20dp"
						android:layout_height="wrap_content"
						android:layout_marginEnd="11dp"
						android:orientation="vertical">

						<LinearLayout
							android:id="@+id/rqothpui3ejf"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:orientation="vertical">

							<ImageView
								android:id="@+id/searchBarIconImage"
								android:layout_width="match_parent"
								android:layout_height="22dp"
								android:scaleType="fitXY"
								android:src="@drawable/searchicon" />
						</LinearLayout>
					</LinearLayout>

				</LinearLayout>

				<EditText
					android:id="@+id/ryxljljb9ai"
					android:layout_width="0dp"
					android:layout_height="wrap_content"
					android:layout_weight="1"
					android:background="@null"
					android:hint="Search"
					android:inputType="text"
					android:paddingVertical="3dp"
					android:textColor="#FFFFFF"
					android:textSize="17sp" />

			</LinearLayout>

			<ImageView
				android:id="@+id/mainFoodSelectionImage"
				android:layout_width="match_parent"
				android:layout_height="397dp"
				android:layout_marginHorizontal="12dp"
				android:layout_marginBottom="6dp"
				android:scaleType="fitXY"
				android:src="@drawable/s1a1a1asw16cr39b2e2e2e" />

			<!-- Buttons with image -->

			<LinearLayout
				android:id="@+id/ri1z68hlv8e"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:gravity="center_vertical"
				android:layout_marginBottom="7dp"
				android:layout_marginHorizontal="40dp"
				android:orientation="horizontal">

				<ImageView
					android:id="@+id/barcodeScannerIconImage"
					android:layout_width="52dp"
					android:layout_height="51dp"
					android:layout_marginEnd="43dp"
					android:scaleType="fitXY"
					android:src="@drawable/macrosicon" />

				<ImageView
					android:id="@+id/DailyLogsIconImage"
					android:layout_width="59dp"
					android:layout_height="51dp"
					android:layout_marginLeft="43dp"
					android:layout_marginRight="27dp"
					android:scaleType="fitXY"
					android:src="@drawable/scanicon" />

				<ImageView
					android:id="@+id/macrosIconImage"
					android:layout_width="59dp"
					android:layout_height="51dp"
					android:layout_marginLeft="48dp"
					android:scaleType="fitXY"
					android:src="@drawable/logsicon" />
			</LinearLayout>
			<LinearLayout
				android:id="@+id/rjte2b9bfe9g"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:gravity="center_vertical"
				android:layout_marginHorizontal="51dp"
				android:orientation="horizontal">
				<TextView
					android:id="@+id/macroTextView"
					android:textColor="#FFFFFF"
					android:textSize="11sp"
					android:layout_height="wrap_content"
					android:layout_width="wrap_content"
					android:text="Macros"
				/>

				<View
					android:layout_width="0dp"
					android:layout_height="3dp"
					android:layout_weight="1" />

				<TextView
					android:id="@+id/barcodeTextView"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="Scan"
					android:textColor="#FFFFFF"
					android:textSize="11sp" />

				<View
					android:layout_width="3dp"
					android:layout_height="1dp"
					android:layout_weight="1" />

				<TextView
					android:id="@+id/logsTextView"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="Logs"
					android:textColor="#FFFFFF"
					android:textSize="13sp" />
			</LinearLayout>
		</LinearLayout>
	</ScrollView>
</LinearLayout>
